<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" id="main" xmlns="http://javafx.com/javafx/8.0.65"
            fx:controller="DBapp.Controllers.addEmployeeController">
   <center>
      <GridPane maxHeight="-Infinity" maxWidth="-Infinity">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="First:" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Last" GridPane.rowIndex="1" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Address:" GridPane.rowIndex="2" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="City" GridPane.rowIndex="3" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="State" GridPane.rowIndex="4" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Zip" GridPane.rowIndex="5" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Phone" GridPane.rowIndex="6" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Email" GridPane.rowIndex="7" />
            <TextField fx:id="first" GridPane.columnIndex="1" />
            <TextField fx:id="last" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="address" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="city" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="state" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="zip" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField fx:id="phone" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <TextField fx:id="email" GridPane.columnIndex="1" GridPane.rowIndex="7" />
          <Button fx:id="submit" mnemonicParsing="false" onMouseClicked="#addEmployee" text="Submit"
                  GridPane.columnIndex="1" GridPane.rowIndex="8"/>
      </GridPane>
   </center>
   <bottom>
      <TextFlow BorderPane.alignment="CENTER">
            <Text fx:id="message" strokeType="OUTSIDE" strokeWidth="0.0" />
      </TextFlow>
   </bottom>
   <top>
      <HBox id="label" BorderPane.alignment="CENTER">
            <TextFlow>
                  <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Add Employee">
                     <font>
                        <Font name="Arial Bold" size="24.0" />
                     </font>
                  </Text>
            </TextFlow>
         <padding>
            <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
         </padding>
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin>
      </HBox>
   </top>
</BorderPane>
